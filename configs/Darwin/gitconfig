[include]
  path = ~/scripts/configs/gitconfig
  path = ~/work/.gitconfig
[credential]
	helper = osxkeychain
[core]
	autocrlf = input
[init]
	defaultBranch = integration
	templatedir = /Users/kellystannard/scripts/git_template_ruby
[alias]
	gone = ! git for-each-ref --format '%(refname:short) %(upstream:track)' | awk '$2 == \"[gone]\" {print $1}' | xargs -r git branch -D
