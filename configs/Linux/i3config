  bindsym $mod+8 mode "$mode_project_2";workspace 45; exec --no-startup-id echo 45 > ~/.i3_workspace;exec --no-startup-id echo Project_2 > ~/.i3_project;exec --no-startup-id ln -sf ~/.config/i3/Project_2_config.sh ~/.current_config.sh
  bindsym $mod+7 mode "$mode_project_1";workspace 55; exec --no-startup-id echo 55 > ~/.i3_workspace;exec --no-startup-id echo Project_1 > ~/.i3_project;exec --no-startup-id ln -sf ~/.config/i3/Project_1_config.sh ~/.current_config.sh
